class A {
    int a;
    
    void setIntA(int p){
        a = p;
    }
    
    void add(int b){
        int c = 3;
        int d = 0; 
        int e = 6;         
        // a = a + (b - c * 0) => a = a + b
        a = a + (b - c * (d/e)); 
    }
}
{
